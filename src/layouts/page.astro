---
import type { MainMenuProps } from '@/components/main-menu.tsx';
import { MainMenu } from '@/components/main-menu.tsx';
import HTML5 from '@/layouts/html5.astro';
interface Props {
  menuItems: MainMenuProps['nodes'];
}
const {
  menuItems
} = Astro.props
---
<HTML5>
  <div class="container mx-auto">
    <div class="flex align-middle">
      <MainMenu client:load nodes={menuItems} />
    </div>
    <slot />
  </div>
</HTML5>